{{> frm_std_function}}
{{> subject_blk}}
{{> frm_list_std}}

  <table id="tableData" name="tableData" class="table table-bordered table-hover table-condensed ">

    {{> std_tab_hdr}}
    {{#each obj}}

    <tr bgcolor="{{this.bg}}" id="myTable" name="myTable">
      <td><button id="btn1" class="glyphicon glyphicon-pencil btn btn-xs"
          onclick='replaceClass("/lib/book/enter?id={{this._id}}")'></button></td>
      <td id="ID">{{this.ID}}<input type="hidden" id="_id" name="_id" value="{{this._id}}" /></td>
      <td id="name" name="name">{{this.name}}</td>
      <td id="SID">{{this.SID}}</td>
      <td id="value">{{this.value}}</td>
      <td id="class">{{this.class}}</td>
      <td id="timestamp" name="timestamp">{{this.ti}}</td>
      <td width=10%>
        <button id="btn2" title="Kopieren des Eintrags"class="glyphicon glyphicon-duplicate btn btn-xs" onclick='replaceClass("/lib/book/copy?id={{this._id}}")'></button>
        <button id="btn3" title="Löschen des Eintrags" class="glyphicon glyphicon-trash btn btn-xs" onclick='deleteClass("{{this._id}}")'></button>
        <button id="btn4" title="Vorgänze zu diesem Buch" class="glyphicon glyphicon-eye-open btn btn-xs" onclick='replaceClass("/lib/transaction/list?BID={{this.ID}}")'></button>
       </td>
    </tr>
    {{/each}}

  </table>
  
  {{frm_footer_list}}

</div>