{{!< layout}}

  <div style="border-radius: 20px; background-color:LightBlue; padding-top:10px; padding-bottom:20px" class="container">
  <h3>{{title}}</h3>
<p>{{obj._id}}</p>


        <div class="col-lg-10" >
          <div class="form-group">
            <label class="control-label col-sm-2">Name:</label>
            <div class="col-sm-10">
              <p>{{obj.name}} / ({{obj.email}}) </p>
            </div>
          </div>

          <div class="form-group">
            <label class="control-label col-sm-2">Day of Arival:</label>
            <div class="col-sm-10">
              <p>{{obj.eta}}</p>
            </div>
          </div>

            <div class="form-group">
              <label class="control-label col-sm-2">Registration:</label>
              <div class="col-sm-10">
                <p>{{obj.registration}}</p>
              </div>
            </div>

            <div class="form-group">
              <label class="control-label col-sm-2">Type:</label>
              <div class="col-sm-10">
                <p>{{obj.value}}</p>
              </div>
            </div>

            <div class="form-group">
              <label class="control-label col-sm-2">MTOW:</label>
              <div class="col-sm-10">
                <p>{{obj.mtow}}</p>
              </div>
            </div>

            <div class="form-group">
               <p id=inputselector name=inputselector>.</p>
            </div>

            <div class="form-group">
              <label class="control-label col-sm-2">QR</label>
              <div class="col-sm-10">
                <img id="qr_link" src='/qrhttp://fly4fun.ddns.info:3001/se/{{obj._id}}'>
              </div>
            </div>


            <div class="form-group">
                <div class="col-sm-10">
                    <button class="btn btn-primary" value="1">OK</button>
                    <a href="/pilots/home" id="cancel" name="cancel" class="btn btn-default">Cancel</a>
                </div>
            </div>
        </div>

  </div>
